<script>
$( "#dropdownCoffee" ).click(function() {
	$("#choicesCoffee").slideToggle();
});
$( "#dropdownEquipment" ).click(function() {
	$("#choicesEquipment").slideToggle();
});
$( "#dropdownDrinkware" ).click(function() {
